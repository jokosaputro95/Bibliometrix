@article{ WOS:000895195600001,
Author = {Alsulami, Abdulaziz A. and Abu Al-Haija, Qasem and Tayeb, Ahmad and
   Alqahtani, Ali},
Title = {An Intrusion Detection and Classification System for IoT Traffic with
   Improved Data Engineering},
Journal = {APPLIED SCIENCES-BASEL},
