@article{ WOS:001249894400001,
Author = {Zuo, Fengqin and Zhang, Damin and Li, Lun and He, Qing and Deng, Jiaxin},
Title = {GSOOA-1DDRSN: Network traffic anomaly detection based on deep residual
   shrinkage networks},
Journal = {HELIYON},
Year = {2024},
