@article{ WOS:000719139700001,
Author = {Ramadan, Rabie A. and Emara, Abdel-Hamid and Al-Sarem, Mohammed and
   Elhamahmy, Mohamed},
Title = {Internet of Drones Intrusion Detection Using Deep Learning},
