@article{ WOS:000565599900001,
Author = {Dutta, Vibekananda and Choras, Michal and Pawlicki, Marek and Kozik,
   Rafal},
Title = {A Deep Learning Ensemble for Network Anomaly and Cyber-Attack Detection},
