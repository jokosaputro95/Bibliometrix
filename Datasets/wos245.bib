@article{ WOS:000779567700008,
Author = {Alshammri, Ghalib H. and Samha, Amani K. and Hemdan, Ezz El-Din and
   Amoon, Mohammed and El-Shafai, Walid},
Title = {An Efficient Intrusion Detection Framework in Software-Defined
   Networking for Cybersecurity Applications},
Journal = {CMC-COMPUTERS MATERIALS \& CONTINUA},
