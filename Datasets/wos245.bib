@article{ WOS:001122401600001,
Author = {Alruwaili, Fahad F. and Asiri, Mashael M. and Alrayes, Fatma S. and
   Aljameel, Sumayh S. and Salama, Ahmed S. and Hilal, Anwer Mustafa},
Title = {Red Kite Optimization Algorithm With Average Ensemble Model for
   Intrusion Detection for Secure IoT},
