@article{ WOS:001199273300001,
Author = {Ullah, Imtiaz and Ullah, Ayaz and Sajjad, Mazhar},
Title = {Towards a Hybrid Deep Learning Model for Anomalous Activities Detection
   in Internet of Things Networks},
