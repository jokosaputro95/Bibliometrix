@ARTICLE{Ahmad2021,
author={Ahmad, Z. and Khan, A.S. and Nisar, K. and Haider, I. and Hassan, R. and Haque, M.R. and Tarmizi, S. and Rodrigues, J.J.P.C.},
title={Anomaly detection using deep neural network for iot architecture},
