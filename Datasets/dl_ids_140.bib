@article{ WOS:000977609600001,
Author = {Xie, Yuyuan and Wang, Maoning and Zhong, Yuzhong and Deng, Lin and
   Zhang, Jianwei},
Title = {Label-Free Anomaly Detection Using Distributed Optical Fiber Acoustic
   Sensing},
