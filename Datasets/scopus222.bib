@ARTICLE{Caville2022,
author={Caville, E. and Lo, W.W. and Layeghy, S. and Portmann, M.},
title={Anomal-E: A self-supervised network intrusion detection system based on graph neural networks},
journal={Knowledge-Based Systems},
year={2022},
volume={258},
