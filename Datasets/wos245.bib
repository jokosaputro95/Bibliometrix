@article{ WOS:000870223000001,
Author = {Siddiqi, Murtaza Ahmed and Pak, Wooguil},
Title = {Tier-Based Optimization for Synthesized Network Intrusion Detection
   System},
Journal = {IEEE ACCESS},
Year = {2022},
Volume = {10},
Pages = {108530-108544},
Abstract = {The innovation and evolution of hacking methodologies have led to a
   sharp rise in cyber attacks, highlighting the need for enhanced network
   security approaches. Network intrusion detection systems based on
   machine learning are playing a significant role in the domain of network
   security. However, designing an optimal framework for a network
   intrusion detection system is an ongoing concern. In this study, an
   optimal framework for a network intrusion detection system based on
   image processing is proposed. The framework is a fusion of augmented
   feature selection flow with an image transformation and enhancement
   methodology. Initially, the proposed framework reduces the number of
   features to achieve overall efficiency. Later, the non-image data is
   transformed into images. The transformed images are then enhanced for
   achieving effective anomaly detection based on a deep-learning
   classifier. The proposed method is implemented on three diverse
   benchmark datasets of intrusion detection. To illustrate the efficiency
   of the proposed framework it is compared with some of the most recent
   publications on image-processing-based network intrusion detection
   systems.},
