@article{ WOS:001161498300006,
Author = {Eljialy, A. E. M. and Uddin, Mohammed Yousuf and Ahmad, Sultan},
Title = {Novel Framework for an Intrusion Detection System Using Multiple Feature
   Selection Methods Based on Deep Learning},
Journal = {TSINGHUA SCIENCE AND TECHNOLOGY},
