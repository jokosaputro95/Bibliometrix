@ARTICLE{Alkahtani2022,
author={Alkahtani, H. and Aldhyani, T.H.H.},
title={Developing Cybersecurity Systems Based on Machine Learning and Deep Learning Algorithms for Protecting Food Security Systems: Industrial Control Systems},
