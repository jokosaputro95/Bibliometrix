@ARTICLE{Lundberg2022102831,
author={Lundberg, H. and Mowla, N.I. and Abedin, S.F. and Thar, K. and Mahmood, A. and Gidlund, M. and Raza, S.},
title={Experimental Analysis of Trustworthy In-Vehicle Intrusion Detection System Using eXplainable Artificial Intelligence (XAI)},
journal={IEEE Access},
year={2022},
volume={10},
