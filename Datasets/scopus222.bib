@ARTICLE{Gimenez2021,
author={Gimenez, P.-F. and Roux, J. and Alata, E. and Auriol, G. and Kaaniche, M. and Nicomette, V.},
title={RIDS: Radio Intrusion Detection and Diagnosis System for Wireless Communications in Smart Environment},
