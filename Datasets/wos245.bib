@article{ WOS:000979168100001,
Author = {Qazi, Emad Ul Haq and Faheem, Muhammad Hamza and Zia, Tanveer},
Title = {HDLNIDS: Hybrid Deep-Learning-Based Network Intrusion Detection System},
