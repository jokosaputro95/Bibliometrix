@article{ WOS:001245419400001,
Author = {Gao, Fei and Liu, Jinshuo and Liu, Yingqi and Gao, Zhenhai and Zhao, Rui},
Title = {Multi-Attack Intrusion Detection for In-Vehicle CAN-FD Messages},
